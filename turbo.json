{
	"$schema": "https://turborepo.org/schema.json",
	"baseBranch": "origin/master",
	"globalDependencies": ["./tsconfig.base.json"],
	"pipeline": {
		"build": {
			"dependsOn": ["^build"],
			"outputs": [".dist/**"]
		},
		"test": {
			"dependsOn": ["^build"],
			"outputs": []
		},
		"eslint": {
			"dependsOn": ["^build"],
			"outputs": []
		},
		"dev": {
			"cache": false
		}
	}
}
