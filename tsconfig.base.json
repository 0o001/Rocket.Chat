{
	"compileOnSave": false,
	"compilerOptions": {
		"target": "es5",
		"module": "ESNext",
		"declaration": true,
		"declarationMap": true,
		"sourceMap": true,
		"strict": true,
		"esModuleInterop": true,
		"skipLibCheck": true,
		"forceConsistentCasingInFileNames": true,
		"moduleResolution": "node",
		"resolveJsonModule": true,
		"jsx": "react-jsx",
		"importsNotUsedAsValues": "error"
	},
	"exclude": ["node_modules", "tmp"]
}
